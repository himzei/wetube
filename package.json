  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --ignore 'scss' --ignore 'static', ",
    "dev:gulp": "cd src && gulp dev",
    "build:gulp": "cd src && gulp deploy",
    "build:server": "babel src --out-dir build --ignore 'src/static' --ignore 'src/assets' --ignore 'src/gulpfile.babel.js'",
    "build": "yarn build:server && yarn build:gulp && yarn copyAll",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "prebuild": "rm -rf build",
    "tunnel": "lt --port 4000", 
    "start": "node build/init.js", 
    "prestart": "yarn build"
  },
